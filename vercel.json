{
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ],
    "crons": [
        {
            "path": "/cron/digest",
            "schedule": "* * * * *"
        }
    ]
}